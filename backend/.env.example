# Railway Traffic Management Backend - Environment Variables
# Copy this file to .env and update the values for your environment

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=railway_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_here
DATABASE_URL=postgresql+psycopg2://postgres:your_password@localhost:5432/railway_db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters
JWT_ALGORITHM=HS256
JWT_EXPIRES_IN=3600

# API Configuration
API_BASE_URL=http://localhost:8001
FRONTEND_URL=http://localhost:5173

# External Services (Optional)
THIRD_PARTY_API_KEY=your_api_key_here
ANALYTICS_KEY=your_analytics_key_here