# Railway AI Integration Environment Configuration
# Copy this to .env and modify as needed

# Core AI Settings
ENABLE_AI_OPTIMIZATION=true
TRAIN_RL_ON_STARTUP=false
RL_TRAINING_EPISODES=300

# Performance Settings
MAX_OPTIMIZATION_TIMEOUT=15.0
MAX_CONCURRENT_OPTIMIZATIONS=5

# Integration Settings
AI_CONFIDENCE_THRESHOLD=0.7
FALLBACK_TO_MANUAL=true

# Data Mapping Settings
CARGO_VALUE_PER_TON=500.0
DEFAULT_PASSENGER_COUNT=150

# Logging Settings
AI_LOG_LEVEL=INFO
ENABLE_AI_METRICS=true

# Safety Settings
REQUIRE_HUMAN_APPROVAL=false
AUTO_EXECUTE_THRESHOLD=0.9

# Existing Railway System Settings (keep your current values)
# DATABASE_URL=postgresql://user:pass@localhost:5432/railway_db
# REDIS_URL=redis://localhost:6379/0
# SECRET_KEY=your_secret_key_here